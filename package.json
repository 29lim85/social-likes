{
  "name": "social-likes",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "autoprefixer-stylus": "0.3.0",
    "babel": "~6.0.15",
    "babel-cli": "~6.1.1",
    "babel-core": "~6.0.20",
    "babel-loader": "~6.0.1",
    "babel-preset-es2015": "~6.0.15",
    "grunt": "~0.4.5",
    "grunt-contrib-imagemin": "~0.8.1",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-contrib-stylus": "~0.20.0",
    "grunt-contrib-uglify": "~0.6.0",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-csso": "~0.6.3",
    "grunt-jscs-checker": "~0.7.1",
    "grunt-webfont": "~0.4.8",
    "load-grunt-tasks": "~0.6.0",
    "rimraf": "~2.4.3",
    "webpack": "~1.12.2"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/social-likes.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js dist/social-likes.min.js --config webpack.config.production.js",
    "clean": "rimraf dist lib",
    "#lint": "eslint src",
    "#prepublish": "npm run lint && npm run test && npm run clean && npm run build",
    "#test": "mocha --compilers js:babel/register test"
  }
}
